// ShapesWithAsterisks.java
// Anser to exercise 2.18 from Deitel How to program Java

public class ShapesWithAsterisks
{
   public static void main( String[] args )
   {
      System.out.println( "*********      ***       *        *" );
      System.out.println( "*       *    *     *    ***      * *" );
      System.out.println( "*       *   *       *  *****    *   *" );
      System.out.println( "*       *   *       *    *     *     *" );
      System.out.println( "*       *   *       *    *    *       *" );
      System.out.println( "*       *   *       *    *     *     *" );
      System.out.println( "*       *   *       *    *      *   *" );
      System.out.println( "*       *    *     *     *       * *" );
      System.out.println( "*********      ***       *        *" );
   }
}
